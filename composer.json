{
  "name": "new-inventor/data-structure",
  "license": "MIT",
  "description": "Property bag with automatic value normalizing and formatting",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1",
    "new-inventor/php-type-checker": "^5.0",
    "psr/simple-cache": "1.*"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "NewInventor\\PropertyBag\\": "src/",
      "TestsPropertyBag\\": "tests/classes/PropertyBag"
    }
  },
  "require-dev": {
    "codeception/codeception": "2.3.*",
    "phpunit/phpunit": "^5.7",
    "symfony/cache": "^3.3",
    "symfony/filesystem": "3.3.*"
  },
  "suggest": {
    "symfony/console": "3.3.*",
    "symfony/yaml": "3.3.*"
  }
}
